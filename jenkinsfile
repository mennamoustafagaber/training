node {
    def app

    stage('Clone repository') {
               checkout scm
    }
    
    stage('build image') {
                 app = docker.build("mennabelalesraa/example-app")
    }    

    stage('push image') {
                          docker.withRegistry('https://registry.hub.docker.com', 'docker-hub-credentials') {
                           app.push("latestnew")
                         }
    }
}

